<h1>New expense</h1>

<% form_for(@expense) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= label :item, :title %><br />
    <%= text_field_with_auto_complete :expense, :item_title %>
  </p>
  <p>
    <%= f.label :amount %><br />
    <%= f.text_field :amount %>
  </p>
  <p>
    <%= f.label :comment %><br />
    <%= f.text_field :comment %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', expenses_path %>
